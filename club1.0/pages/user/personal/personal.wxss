/* pages/user/personal/personal.wxss */
@import "../../inc/inc.wxss";
 .head{
   width: 100%;
    /* height: 354rpx;  */
    min-height: 232rpx;
    padding-top: 30rpx;
  background:#3399ff;
   text-align: center;
 }
 .head-img{
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    display: inline-block;
     /* margin-top: 60rpx;  */
    overflow: hidden;
 }
 .head-name{
   color: #fff;
   font-size: 36rpx;
   line-height: 60rpx;
 }
 .head-info{
     display: flex;
     justify-content: center;
      height:69rpx; 
      margin-top: 24rpx; 
      padding-bottom: 40rpx;

 }
 .head-info-left{
   flex: 1;
   font-size:24rpx;
color:#cce7ff;
border-right:1rpx solid #7FC0FF;
 }
  .head-info-left:nth-last-child(1){
      border: none;
  }
  .head-info-right{
      font-size:36rpx;
color:#ffffff;
  }